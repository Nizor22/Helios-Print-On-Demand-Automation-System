# =============================================================================
# OPTIONAL: PERFORMANCE THRESHOLDS
# =============================================================================
# Minimum opportunity score (0-10 scale, default: 7.0)
MIN_OPPORTUNITY_SCORE=7.0

# Minimum audience confidence (0-10 scale, default: 7.0)
MIN_AUDIENCE_CONFIDENCE=7.0

# Minimum profit margin (0.0-1.0, default: 0.35 = 35%)
MIN_PROFIT_MARGIN=0.35

# Maximum execution time in seconds (default: 300 = 5 minutes)
MAX_EXECUTION_TIME=300

# =============================================================================
# OPTIONAL: BEHAVIOR SETTINGS
# =============================================================================
# Default product margin (0.0-1.0, default: 0.5 = 50%)
DEFAULT_MARGIN=0.5

# Default product colors (comma-separated)
DEFAULT_COLORS=white,black,navy,red

# Default product sizes (comma-separated)
DEFAULT_SIZES=S,M,L,XL,2XL

# Create products as drafts by default (true/false)
DEFAULT_DRAFT=true

# Run in dry-run mode (true/false)
DRY_RUN=true

# Allow live publishing to stores (true/false)
ALLOW_LIVE_PUBLISHING=false

# =============================================================================
# OPTIONAL: OPTIMIZATION SETTINGS
# =============================================================================
# Enable parallel processing (true/false)
ENABLE_PARALLEL_PROCESSING=true

# Enable batch creation (true/false)
ENABLE_BATCH_CREATION=true

# Enable adaptive learning (true/false)
ENABLE_ADAPTIVE_LEARNING=true

# Enable auto-optimization (true/false)
ENABLE_AUTO_OPTIMIZATION=true

# =============================================================================
# OPTIONAL: GOOGLE CLOUD INFRASTRUCTURE
# =============================================================================
# Vertex AI Configuration
VERTEX_AI_PROJECT_ID=your-vertex-ai-project-id
VERTEX_AI_LOCATION=us-central1

# =============================================================================
# OPTIONAL: GEMINI AI MODELS
# =============================================================================
# Override default Gemini models if needed
GEMINI_MODEL=gemini-2.0-flash-exp
GEMINI_PRO_MODEL=gemini-1.5-pro
GEMINI_FLASH_MODEL=gemini-1.5-flash
GEMINI_ULTRA_MODEL=gemini-1.0-ultra

# =============================================================================
# OPTIONAL: IMAGE GENERATION
# =============================================================================
# Image generation settings
IMAGEN_MODEL=imagen-3
IMAGE_RESOLUTION=1024x1024
IMAGE_FORMAT=PNG
IMAGE_QUALITY=high

# =============================================================================
# OPTIONAL: MCP TOOLS CONFIGURATION
# =============================================================================
# Enable/disable specific MCP tools
ENABLE_GOOGLE_TRENDS_TOOL=true
ENABLE_SOCIAL_MEDIA_SCANNER=true
ENABLE_NEWS_ANALYZER=true
ENABLE_COMPETITOR_INTELLIGENCE=true

# =============================================================================
# OPTIONAL: PERFORMANCE MONITORING
# =============================================================================
# Enable performance monitoring features
ENABLE_PERFORMANCE_MONITORING=true
ENABLE_CLOUD_MONITORING=true
ENABLE_CLOUD_LOGGING=true
ENABLE_CLOUD_TRACE=true

# =============================================================================
# OPTIONAL: CACHING CONFIGURATION
# =============================================================================
# Enable Redis caching
ENABLE_REDIS_CACHING=true

# Cache TTL settings (in seconds)
CACHE_TTL_TREND_DATA=3600        # 1 hour
CACHE_TTL_PRINTIFY_CATALOG=86400 # 24 hours
CACHE_TTL_API_RESPONSES=300      # 5 minutes

# =============================================================================
# OPTIONAL: EXTERNAL API INTEGRATIONS
# =============================================================================
# SERP API for search results
SERPAPI_API_KEY=your_serpapi_key_here

# =============================================================================
# DEVELOPMENT & TESTING
# =============================================================================
# Set to false in production
DRY_RUN=true
ALLOW_LIVE_PUBLISHING=false

# =============================================================================
# NOTES:
# =============================================================================
# 1. Replace all "your_*_here" values with your actual credentials
# 2. Keep this file secure and never commit it to version control
