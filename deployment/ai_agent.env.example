# Helios Hybrid AI System Environment Configuration
# Copy this file to .env and configure with your values

# Core AI Configuration
GOOGLE_MCP_URL=https://your-mcp-server.run.app
GOOGLE_MCP_AUTH_TOKEN=your_auth_token
GEMINI_API_KEY=your_gemini_api_key

# Hybrid AI System Configuration
ENABLE_HYBRID_AI_SYSTEM=true
ENABLE_THINKING_BUDGET_CONTROL=true
ENABLE_COST_OPTIMIZATION=true

# AI Model Selection Strategy
AI_MODEL_SELECTION_STRATEGY=hybrid_cost_optimized
DEFAULT_THINKING_BUDGET=10000
MAX_THINKING_BUDGET_PER_DAY=100000

# Hybrid Processing Settings
ENABLE_AI_RULES_VALIDATION=true
ENABLE_QUALITY_GATES=true
ENABLE_BUSINESS_RULES_ENGINE=true

# Cost Optimization Thresholds
AI_COST_PER_DAY_TARGET=150.0
AI_COST_PER_PRODUCT_TARGET=5.0
TOKEN_UTILIZATION_EFFICIENCY_TARGET=0.85

# Hybrid Agent Configuration
CEO_THINKING_BUDGET=10000
PRODUCT_STRATEGIST_THINKING_BUDGET=8000
AUDIENCE_ANALYST_THINKING_BUDGET=5000
CREATIVE_DIRECTOR_THINKING_BUDGET=3000
MARKETING_COPYWRITER_THINKING_BUDGET=2000
ETHICAL_GUARDIAN_THINKING_BUDGET=1000

# Gemini 2.5 Models
GEMINI_2_5_FLASH_LITE=gemini-2.5-flash-lite
GEMINI_2_5_FLASH_THINKING=gemini-2.5-flash-thinking
GEMINI_2_0_FLASH_LITE=gemini-2.0-flash-lite

# AI Features
USE_AI_AGENT=true
USE_AI_ORCHESTRATION=true
USE_AI_INSIGHTS=true

# AI Thresholds
AI_CONFIDENCE_THRESHOLD=0.7
AI_PATTERN_RECOGNITION_THRESHOLD=0.8
AI_PREDICTION_CONFIDENCE_THRESHOLD=0.75

# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_CLOUD_REGION=us-central1
GOOGLE_CLOUD_LOCATION=us-central1

# Vertex AI Configuration
VERTEX_AI_PROJECT_ID=your-project-id
VERTEX_AI_LOCATION=us-central1

# MCP Tools Configuration
ENABLE_GOOGLE_TRENDS_TOOL=true
ENABLE_SOCIAL_MEDIA_SCANNER=true
ENABLE_NEWS_ANALYZER=true
ENABLE_COMPETITOR_INTELLIGENCE=true

# MCP Tool Rate Limits
GOOGLE_TRENDS_RATE_LIMIT=100
SOCIAL_MEDIA_RATE_LIMIT=200
NEWS_ANALYZER_RATE_LIMIT=150

# Performance Monitoring
ENABLE_PERFORMANCE_MONITORING=true
ENABLE_CLOUD_MONITORING=true
ENABLE_CLOUD_LOGGING=true
ENABLE_CLOUD_TRACE=true

# Caching Configuration
ENABLE_REDIS_CACHING=true
CACHE_TTL_TREND_DATA=3600
CACHE_TTL_PRINTIFY_CATALOG=86400
CACHE_TTL_API_RESPONSES=300

# Printify Configuration
PRINTIFY_API_TOKEN=your_printify_token
PRINTIFY_SHOP_ID=your_shop_id
PRINTIFY_BASE_URL=https://api.printify.com/v1

# Etsy Configuration
ETSY_API_KEY=your_etsy_api_key
ETSY_BASE_URL=https://openapi.etsy.com/v3

# Performance Thresholds
MIN_OPPORTUNITY_SCORE=5.0
MIN_AUDIENCE_CONFIDENCE=7.0
MIN_PROFIT_MARGIN=0.35
MAX_EXECUTION_TIME=300

# Optimization Settings
ENABLE_PARALLEL_PROCESSING=true
ENABLE_BATCH_CREATION=true
ENABLE_ADAPTIVE_LEARNING=true
ENABLE_AUTO_OPTIMIZATION=true

# Safety Settings
ALLOW_LIVE_PUBLISHING=false

# Debug and Development
DEBUG_MODE=false
ENABLE_VERBOSE_LOGGING=false
LOG_LEVEL=info
DRY_RUN=true
